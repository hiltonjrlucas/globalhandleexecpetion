                    <div class="form-group">
                        <label>Você possui algum tipo de deficiência?</label>
                        <div id="slbDeficiencia"></div>
                    </div>
                </div>
                <div class="col-md-3" id="divDeficiencia" style="display: none">
                    <div class="form-group">
                        <label>Qual?</label>
                        <div id="txtDeficiencia"></div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="box box-widget">
                        <div class="box-header with-border">
                            <span class="box-title" onclick="$('#btnFilhos').click()">Filhos</span>
                            <div class="box-tools pull-right">
                                <button id="btnFilhos" type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip">
                                    <i class="fa fa-minus"></i>
                                </button>
                            </div>
                        </div>
                        <div class="box-body">
                            <div class="row">
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label>Tem Filhos(s)?</label>
                                        <div id="slbFilho"></div>
                                    </div>
                                </div>
                                <div class="col-md-2" id="divNumeroFilho" style="display: none">
                                    <div class="form-group">
                                        <label>Quantos?</label>
                                        <div id="slbNumeroFilho"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="row" id="divFilhos"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="box-footer">
            <div id="divIdentificacaoValidacoes"></div>
            <div id="btnIdentificacao" class="pull-right"></div>
        </div>
